<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.jinliyu.shoppingapp_1.HomepageActivity"
    tools:layout_editor_absoluteY="81dp"
    tools:showIn="@layout/app_bar_homepage">

    <ViewFlipper
        android:id="@+id/flipper"
        android:layout_width="wrap_content"
        android:layout_height="204dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">


        <ImageView
            android:layout_width="match_parent"
            android:layout_height="184dp"
            android:src="@drawable/flip2"
            tools:layout_editor_absoluteX="8dp"
            tools:layout_editor_absoluteY="164dp" />

        <ImageView
            android:layout_width="368dp"
            android:layout_height="wrap_content"
            android:src="@drawable/flip4"
            tools:layout_editor_absoluteX="8dp"
            tools:layout_editor_absoluteY="0dp" />

        <ImageView
            android:layout_width="382dp"
            android:layout_height="184dp"
            android:src="@drawable/flip1"
            tools:layout_editor_absoluteX="8dp"
            tools:layout_editor_absoluteY="0dp" />
    </ViewFlipper>


    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="204dp"
        android:layout_height="244dp"
        android:src="@drawable/home2"
        tools:layout_editor_absoluteY="306dp" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="189dp"
        android:layout_height="157dp"
        android:src="@drawable/home3"
        tools:layout_editor_absoluteX="195dp"
        tools:layout_editor_absoluteY="348dp" />

    <ImageView

        android:id="@+id/homeicon1"
        android:layout_width="136dp"
        android:layout_height="115dp"

        app:srcCompat="@drawable/homeicon1"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="223dp" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="137dp"
        android:layout_height="116dp"
        app:srcCompat="@drawable/homeicon2"
        tools:layout_editor_absoluteX="128dp"
        tools:layout_editor_absoluteY="223dp" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="123dp"
        android:layout_height="118dp"
        app:srcCompat="@drawable/homeicon3"
        tools:layout_editor_absoluteX="258dp"
        tools:layout_editor_absoluteY="223dp" />


</android.support.constraint.ConstraintLayout>
